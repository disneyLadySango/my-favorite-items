type Query {
    getWorkProduct(id: ID!): WorkProduct
}

type WorkProduct {
    id: ID!
    name: String!
}

type Mutation {
    registWorkProduct(name: String!, description: String!, releaseDate: String!): RegistWorkProductResponse
}

type RegistWorkProductResponse {
    name: String!,
    isRegist: Boolean!,
    errorMessage: String
}